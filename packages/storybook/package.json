{
  "name": "@krofdrakula/prop-docs-storybook",
  "version": "0.0.6",
  "author": "Klemen Slaviƒç <krof.drakula@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KrofDrakula/prop-docs/issues"
  },
  "homepage": "https://github.com/KrofDrakula/prop-docs#readme",
  "description": "A tool to translate TS types to Storybook ArgTypes",
  "keywords": [
    "typescript",
    "types",
    "proptypes",
    "preact"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.umd.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "tsc"
  },
  "peerDependencies": {
    "typescript": ">=4",
    "ts-morph": "^17.0.1"
  },
  "devDependencies": {
    "@krofdrakula/prop-docs-preact": "workspace:*",
    "@storybook/types": "7.0.0-alpha.44",
    "@types/dedent": "0.7.0",
    "dedent": "0.7.0",
    "preact": "10.11.3",
    "ts-morph": "17.0.1",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vite-plugin-dts": "1.7.2",
    "vitest": "0.28.4"
  }
}
